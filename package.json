    "@lexical/plain-text": "^0.8.0",
    "@lexical/react": "^0.8.0",
    "@lexical/rich-text": "^0.8.0",
    "@lexical/selection": "^0.8.0",
    "@lexical/table": "^0.8.0",
    "@lexical/utils": "^0.8.0",
    "@next/font": "^13.1.6",
    "@tanstack/react-query": "^4.24.4",
    "@tanstack/react-query-devtools": "^4.24.4",
