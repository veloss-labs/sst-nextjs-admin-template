{
  "name": "sst-nextjs-template",
  "version": "0.0.1-beta.0",
  "description": "nextjs sst template project",
  "scripts": {
    "dev": "run-s dev:*",
    "dev:cli": "node dist/index.js --env local",
    "dev:start": "next",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "deploy:dev": "run-s sst:dev:*",
    "sst:dev:cli": "node dist/index.js --env dev",
    "sst:dev:deploy": "sst deploy",
    "partytown": "partytown copylib public/~partytown",
    "cli:build": "tsup scripts/cli/index.ts --minify",
    "cli:dev": "tsup scripts/cli/index.ts  --watch",
    "cli:start": "run-s cli:build"
  },
  "keywords": [
    "nextjs",
    "sst",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "dependencies": {
